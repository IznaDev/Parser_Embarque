{
    "target":"test",
    "devices":[
        {
            "id":"test_1",
            "type":"testinput",
            "config":
            {
                "pin":15
            },
            "inputs":["value"]
        },
        {
            "id":"test_2",
            "type":"testinputoutput",
            "config":
            {
                "pin":10,
                "val_init":3
            },
            "inputs":["val"],
            "outputs":["val"]
        },
        {
            "id":"test_3",
            "type":"testoutput",
            "config":
            {
                "pin":12
            },
            "inputs":["value"],
            "outputs":["value"]          
        }
    ],
    "behaviors":[
        {
            "if":"${test_1.value} > 0",
            "then": "${test_3.value} = 255",
            "else": ["${test_3.value} = 0"]
        },
        {
            "if":"${test_2.val} <=0",
            "then": "${test_2.val} += 2",
            "else": ["${test_2.val} -= 2"]
        }
    ]
}